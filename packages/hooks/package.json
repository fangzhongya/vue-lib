{
    "name": "@fangzhongya/vue-lib-hooks",
    "private": false,
    "version": "1.0.22",
    "type": "module",
    "description ": "vue组件库use",
    "author": "fangzhongya ",
    "license": "MIT",
    "keywords": [
        "vue组件库use"
    ],
    "repository": {
        "type": "git",
        "url": " https://github.com/fangzhongya/vue-lib.git"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "dev:index": "ts-node node/index.ts",
        "dev:tests": "ts-node node/tests.ts",
        "dev:packa": "ts-node node/package.ts",
        "dev:v": "ts-node node/version.ts",
        "build": "tsup",
        "build:all": "pnpm dev:index & pnpm build & pnpm dev:packa",
        "test": "vitest",
        "test:run": "vitest run"
    },
    "devDependencies": {
        "@fangzhongya/create": "0.2.2-36",
        "@types/node": "^18.13.0",
        "ts-node": "^10.9.1",
        "tsup": "^6.6.3",
        "typescript": "^4.9.5",
        "vite": "^4.1.2",
        "vitest": "^0.28.5"
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*"
            ]
        }
    },
    "exports": {
        "./*": "./*",
        "./cssname/index": {
            "require": "./dist/cssname/index.cjs",
            "import": "./dist/cssname/index.js",
            "types": "./dist/cssname/index.d.ts"
        },
        "./default-config/index": {
            "require": "./dist/default-config/index.cjs",
            "import": "./dist/default-config/index.js",
            "types": "./dist/default-config/index.d.ts"
        },
        "./global-config/index": {
            "require": "./dist/global-config/index.cjs",
            "import": "./dist/global-config/index.js",
            "types": "./dist/global-config/index.d.ts"
        },
        "./locale/index": {
            "require": "./dist/locale/index.cjs",
            "import": "./dist/locale/index.js",
            "types": "./dist/locale/index.d.ts"
        },
        "./index": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "module": "./dist/index.js",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ]
}