{
    "name": "@fangzhongya/vue-lib-components",
    "version": "1.0.9",
    "type": "module",
    "description ": "vue组件库工具",
    "author": "fangzhongya ",
    "license": "MIT",
    "keywords": [
        "vue组件库工具"
    ],
    "repository": {
        "type": "git",
        "url": " https://github.com/fangzhongya/vue-lib.git"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "dev:index": "ts-node node/vue.ts",
        "dev:tests": "ts-node node/tests.ts",
        "dev:packa": "ts-node node/package.ts",
        "dev:v": "ts-node node/version.ts",
        "dev:c": "pnpm build",
        "build": "vue-tsc && vite build",
        "build:all": "pnpm build & pnpm dev:packa",
        "test": "vitest",
        "test:run": "vitest run"
    },
    "devDependencies": {
        "@fangzhongya/create": "0.2.2-40",
        "@fangzhongya/vue-lib-locale": "workspace:^1.0.10",
        "@types/node": "^18.14.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "sass": "^1.58.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "vite": "^4.1.2",
        "vite-plugin-dts": "2.0.0-beta.1",
        "vue-tsc": "^1.1.3"
    },
    "dependencies": {
        "vue": "^3.2.47"
    },
    "files": [
        "*.d.ts",
        "dist"
    ],
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "./*": "./*",
        "./components/packages/config-provider/index": {
            "require": "./dist/components/packages/config-provider/index.cjs",
            "import": "./dist/components/packages/config-provider/index.js",
            "types": "./dist/components/packages/config-provider/index.d.ts"
        },
        "./components/packages/forms/form/index": {
            "require": "./dist/components/packages/forms/form/index.cjs",
            "import": "./dist/components/packages/forms/form/index.js",
            "types": "./dist/components/packages/forms/form/index.d.ts"
        },
        "./components/packages/forms/subset/input/index": {
            "require": "./dist/components/packages/forms/subset/input/index.cjs",
            "import": "./dist/components/packages/forms/subset/input/index.js",
            "types": "./dist/components/packages/forms/subset/input/index.d.ts"
        },
        "./components/packages/forms/subset/select/index": {
            "require": "./dist/components/packages/forms/subset/select/index.cjs",
            "import": "./dist/components/packages/forms/subset/select/index.js",
            "types": "./dist/components/packages/forms/subset/select/index.d.ts"
        },
        "./components/packages/index": {
            "require": "./dist/components/packages/index.cjs",
            "import": "./dist/components/packages/index.js",
            "types": "./dist/components/packages/index.d.ts"
        },
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*"
            ]
        }
    }
}