{
    "name": "@fangzhongya/vue-lib-utils",
    "private": false,
    "version": "1.0.11",
    "type": "module",
    "description ": "vue组件库工具",
    "author": "fangzhongya ",
    "license": "MIT",
    "keywords": [
        "vue组件库工具"
    ],
    "repository": {
        "type": "git",
        "url": " https://github.com/fangzhongya/vue-lib.git"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "dev:index": "ts-node node/index.ts",
        "dev:tests": "ts-node node/tests.ts",
        "dev:packa": "ts-node node/package.ts",
        "dev:v": "ts-node node/version.ts",
        "build": "tsup",
        "build:all": "pnpm build & pnpm dev:packa",
        "test": "vitest",
        "test:run": "vitest run"
    },
    "devDependencies": {
        "@fangzhongya/create": "0.2.2-36",
        "@fangzhongya/utils": "0.0.7-11",
        "@types/node": "^18.13.0",
        "ts-node": "^10.9.1",
        "tsup": "^6.6.3",
        "typescript": "^4.9.5",
        "vite": "^4.1.2",
        "vitest": "^0.28.5",
        "vue": "^3.2.47"
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*"
            ]
        }
    },
    "exports": {
        "./*": "./*",
        "./vue/props/index": {
            "require": "./dist/vue/props/index.cjs",
            "import": "./dist/vue/props/index.js",
            "types": "./dist/vue/props/index.d.ts"
        },
        "./vue/props/util": {
            "require": "./dist/vue/props/util.cjs",
            "import": "./dist/vue/props/util.js",
            "types": "./dist/vue/props/util.d.ts"
        },
        "./vue/props/runtime": {
            "require": "./dist/vue/props/runtime.cjs",
            "import": "./dist/vue/props/runtime.js",
            "types": "./dist/vue/props/runtime.d.ts"
        },
        "./vue/props/types": {
            "require": "./dist/vue/props/types.cjs",
            "import": "./dist/vue/props/types.js",
            "types": "./dist/vue/props/types.d.ts"
        },
        "./vue/definePropType": {
            "require": "./dist/vue/definePropType.cjs",
            "import": "./dist/vue/definePropType.js",
            "types": "./dist/vue/definePropType.d.ts"
        },
        "./vue/index": {
            "require": "./dist/vue/index.cjs",
            "import": "./dist/vue/index.js",
            "types": "./dist/vue/index.d.ts"
        },
        "./vue/withInstall": {
            "require": "./dist/vue/withInstall.cjs",
            "import": "./dist/vue/withInstall.js",
            "types": "./dist/vue/withInstall.d.ts"
        },
        "./enums": {
            "require": "./dist/enums.cjs",
            "import": "./dist/enums.js",
            "types": "./dist/enums.d.ts"
        },
        "./getOnlyKey": {
            "require": "./dist/getOnlyKey.cjs",
            "import": "./dist/getOnlyKey.js",
            "types": "./dist/getOnlyKey.d.ts"
        },
        "./index": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "module": "./dist/index.js",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ]
}